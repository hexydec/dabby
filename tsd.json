{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"lib": ["ES2022", "DOM"],
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"baseUrl": ".",
		"paths": {
			"dabbyjs": ["./src/modular.ts"],
			"dabbyjs/*": ["./src/*"]
		}
	}
}
